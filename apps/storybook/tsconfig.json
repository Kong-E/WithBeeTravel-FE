{
  "extends": "@withbee/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "esModuleInterop": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../../packages/ui/**/*",
    "../../packages/ui/custom.d.ts",
    "./custom.d.ts"
  ],
  "exclude": ["node_modules"]
}
